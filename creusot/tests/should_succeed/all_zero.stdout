module Type
  use Ref
  use mach.int.Int
  use mach.int.Int32
  use mach.int.Int64
  use mach.int.UInt32
  use mach.int.UInt64
  use string.Char
  use floating_point.Single
  use floating_point.Double
  use prelude.Prelude
  type core_option_option 't = 
    | Core_Option_Option_None
    | Core_Option_Option_Some('t)
    
  predicate drop_core_option_option (drop_t : 't -> bool) (self : core_option_option 't) = 
    match (self) with
      | Core_Option_Option_None -> true
      | Core_Option_Option_Some(a) -> drop_t a
      end
  type allzero_list  = 
    | AllZero_List_Cons(uint32, allzero_list)
    | AllZero_List_Nil
    
  predicate drop_allzero_list (self : allzero_list) = 
    match (self) with
      | AllZero_List_Cons(a, b) -> drop_uint a && drop_fix b
      | AllZero_List_Nil -> true
      end
end
module AllZero_Len
  use Ref
  use mach.int.Int
  use mach.int.Int32
  use mach.int.Int64
  use mach.int.UInt32
  use mach.int.UInt64
  use string.Char
  use floating_point.Single
  use floating_point.Double
  use prelude.Prelude
  use Type
  let rec function impl (l : Type.allzero_list) : int = 
    match (l) with
      | Type.AllZero_List_Cons(_, ls) -> Int32.to_int (1 : int32) + impl ls
      | Type.AllZero_List_Nil -> Int32.to_int (0 : int32)
      end
end
module AllZero_Get
  use Ref
  use mach.int.Int
  use mach.int.Int32
  use mach.int.Int64
  use mach.int.UInt32
  use mach.int.UInt64
  use string.Char
  use floating_point.Single
  use floating_point.Double
  use prelude.Prelude
  use Type
  let rec function impl (l : Type.allzero_list) (ix : int) : Type.core_option_option uint32 = 
    match (l) with
      | Type.AllZero_List_Cons(x, ls) -> match (ix = Int32.to_int (0 : int32)) with
        | True -> Type.Core_Option_Option_Some(x)
        | False -> impl ls (ix - Int32.to_int (1 : int32))
        end
      | Type.AllZero_List_Nil -> Type.Core_Option_Option_None
      end
end
