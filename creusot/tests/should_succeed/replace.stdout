module Type
  use Ref
  use mach.int.Int
  use mach.int.Int32
  use mach.int.Int64
  use mach.int.UInt32
  use mach.int.UInt64
  use string.Char
  use floating_point.Single
  use floating_point.Double
  use prelude.Prelude
  type core_option_option 't = 
    | Core_Option_Option_None
    | Core_Option_Option_Some('t)
    
  predicate drop_core_option_option (drop_t : 't -> bool) (self : core_option_option 't) = 
    match (self) with
      | Core_Option_Option_None -> true
      | Core_Option_Option_Some(a) -> drop_t a
      end
  type replace_something  = 
    | Replace_Something(uint32, core_option_option (replace_something))
    
  predicate drop_replace_something (self : replace_something) = 
    let Replace_Something(a, b) = self in drop_uint a && drop_core_option_option drop_fix b
end
module Replace_Test
