  use prelude.Prelude
  use Type
  let rec cfg impl (o_a_1 : Type.replace_something) (o_b_2 : Type.replace_something) : () = 
  var _0 : ();
  var a_1 : Type.replace_something;
  var b_2 : Type.replace_something;
  var _3 : Type.replace_something;
  {
    a_1 <- o_a_1;
    b_2 <- o_b_2;
    goto BB0
  }
  BB0 {
    assume { Type.drop_replace_something _3 };
    _3 <- b_2;
    assume { Type.drop_replace_something a_1 };
    a_1 <- _3;
    goto BB1
  }
  BB1 {
    goto BB2
  }
  BB2 {
    _0 <- ();
    goto BB3
  }
  BB3 {
    goto BB4
  }
  BB4 {
    assume { Type.drop_replace_something a_1 };
    return _0
  }
  
end
