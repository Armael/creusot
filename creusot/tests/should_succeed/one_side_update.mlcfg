module Ambient
use Ref 
use mach.int.Int 
use mach.int.Int32
use mach.int.Int64
use mach.int.UInt32
use mach.int.UInt64
use string.Char
use floating_point.Single
use floating_point.Double
(** Generic Type for borrowed values *) 
type borrowed 'a = 
{ current : 'a ; 
final : 'a; (* The "future" value when borrow will end *) 
} 
let function ( *_ ) x = x.current 
let function ( ^_ ) x = x.final 
val borrow_mut (a : 'a) : borrowed 'a 
ensures { *result = a }
  type myInt  =
      | MyInt(usize)

  let cfg main () : ()
    =
  var _0 : ();
  var a_1 : myInt;
  var b_2 : borrowed (myInt);
  var _3 : bool;
  var _4 : bool;
  var _5 : usize;
  var _6 : myInt;
  {
    goto BB0;
  }
  BB0 {
    a_1 <- MyInt(10);
    b_2 <- borrow_mut a_1;
    a_1 <-  ^ b_2;
    _3 <- true;
    switch (_3)
      | False -> goto BB2
      | _ -> goto BB1
      end
  }
  BB1 {
    assume { ^ b_2 = * b_2 };
    goto BB3
  }
  BB3 {
    _5 <- let MyInt(a) = a_1 in a;
    _4 <- _5 = 10;
    _0 <- ();
    goto BB4
  }
  BB4 {
    _0
  }
  BB2 {
    _6 <- MyInt(5);
    b_2 <- { b_2 with current = _6 };
    assume { ^ b_2 = * b_2 };
    _0 <- ();
    goto BB4
  }

end
