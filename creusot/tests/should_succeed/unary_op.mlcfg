module Ambient
  use Ref
  use mach.int.Int
  use mach.int.Int32
  use mach.int.Int64
  use mach.int.UInt32
  use mach.int.UInt64
  use string.Char
  use floating_point.Single
  use floating_point.Double

  type usize = int
  type isize = int

  type borrowed 'a = { current : 'a ; final : 'a; }
  let function ( *_ ) x = x.current
  let function ( ^_ ) x = x.final
  val borrow_mut (a : 'a) : borrowed 'a
  ensures { *result = a }

  predicate drop_fix (_ : 'a) = true
  predicate drop_uint (_ : 'a) = true
  predicate drop_int (_ : 'a) = true
  predicate drop_float (_ : 'a) = true
  predicate drop_bool (_ : 'a) = true
  predicate drop_mut_ref (b : borrowed 'a) = ^ b = * b
  predicate drop_ref (_ : 'a) = true

  scope Type
  end
  let rec cfg main () : ()
    =
  var _0 : ();
  var _1 : ();
  var _2 : bool;
  var _3 : bool;
  var _4 : ();
  {
    goto BB0
  }
  BB0 {
    _3 <- not false;
    _2 <- not _3;
    switch (_2)
      | False -> goto BB2
      | True -> goto BB1
      | _ -> goto BB1
      end
  }
  BB1 {
    assume { drop_bool _2 };
    goto BB3
  }
  BB2 {
    assume { drop_bool _2 };
    _1 <- ();
    assume { (fun tup -> let () = tup in True) _1 };
    _0 <- ();
    return _0
  }
  BB3 {
    absurd
  }


end
